{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "preLaunchTask": "Pre Test",
            "program": "${workspaceFolder}/NetBeauty/src/main",
            "env": {},
            "cwd": "${workspaceFolder}",
            "args": [
                "--loglevel=Info",
                "--usepatch",
                "--enabledebug",
                "--srmode",
                "--gitcdn=https://gitee.com/liesauer/HostFXRPatcher",
                "./NetBeauty/test/",
                "./libraries",
            ],
            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 1024,
                "maxArrayValues": 1000,
                "maxStructFields": -1
            }
        }
    ]
}